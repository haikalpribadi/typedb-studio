        <markup.strike>
            <fontStyle>italic</fontStyle>
            <foreground>#FFB86C</foreground>
        </markup.strike>
        <markup.bold>
            <fontStyle>bold</fontStyle>
            <foreground>#FFB86C</foreground>
        </markup.bold>
        <markup.italic>
            <fontStyle>italic</fontStyle>
            <foreground>#FFB86C</foreground>
        </markup.italic>
        <markup.heading>
            <foreground>#8BE9FD</foreground>
        </markup.heading>
        <punctuation.definition.list_item.markdown>
            <foreground>#FF79C6</foreground>
        </punctuation.definition.list_item.markdown>
        <markup.quote>
            <fontStyle>italic</fontStyle>
            <foreground>#6272A4</foreground>
        </markup.quote>
        <punctuation.definition.blockquote.markdown>
            <fontStyle>italic</fontStyle>
            <background>#6272A4</background>
            <foreground>#6272A4</foreground>
        </punctuation.definition.blockquote.markdown>
        <meta.separator>
            <foreground>#6272A4</foreground>
        </meta.separator>
        <text.html.markdown>
            <foreground>#50FA7B</foreground>
        </text.html.markdown>
        <markup.raw.inline>
            <foreground>#50FA7B</foreground>
        </markup.raw.inline>
        <markup.underline>
            <fontStyle>underline</fontStyle>
            <foreground>#BD93F9</foreground>
        </markup.underline>
        <markup.raw.block>
