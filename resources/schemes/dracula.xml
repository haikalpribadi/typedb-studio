            <background>#ff79c6</background>
            <foreground>#F8F8F0</foreground>
        </invalid>
        <invalid.deprecated>
            <background>#bd93f9</background>
            <foreground>#F8F8F0</foreground>
        </invalid.deprecated>
        <meta.diff>
            <foreground>#6272a4</foreground>
        </meta.diff>
        <meta.diff.header>
            <foreground>#6272a4</foreground>
        </meta.diff.header>
        <markup.deleted>
            <foreground>#ff79c6</foreground>
        </markup.deleted>
        <markup.inserted>
            <foreground>#50fa7b</foreground>
        </markup.inserted>
        <markup.changed>
            <foreground>#E6DB74</foreground>
        </markup.changed>
        <entity.name.filename>
            <foreground>#E6DB74</foreground>
        </entity.name.filename>
        <message.error>
            <foreground>#F83333</foreground>
        </message.error>

