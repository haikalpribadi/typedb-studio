        <support.function>
            <foreground>#8be9fd</foreground>
        </support.function>
        <support.constant>
            <foreground>#6be5fd</foreground>
        </support.constant>
        <support.type>
            <fontStyle>italic</fontStyle>
            <foreground>#66d9ef</foreground>
        </support.type>
        <support.class>
            <fontStyle>italic</fontStyle>
            <foreground>#66d9ef</foreground>
        </support.class>
        <support.other.variable>
        </support.other.variable>
        <support.other.namespace>
            <fontStyle>italic</fontStyle>
            <foreground>#66d9ef</foreground>
        </support.other.namespace>
