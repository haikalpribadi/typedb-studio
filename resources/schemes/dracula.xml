<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <name>Dracula</name>
    <author>Zeno Rocha</author>
    <uuid>83091B89-765E-4F0D-9275-0EC6CB084126</uuid>
    <semanticClass>theme.dracula</semanticClass>
    <colorSpaceName>sRGB</colorSpaceName>
    <globals>
        <background>#282a36</background>
        <caret>#f8f8f0</caret>
        <blockCaret>#999a9e</blockCaret>
        <foreground>#f8f8f2</foreground>
        <invisibles>#3B3A32</invisibles>
        <lineHighlight>#44475a</lineHighlight>
        <selection>#44475a</selection>
        <findHighlight>#effb7b</findHighlight>
        <findHighlightForeground>#000000</findHighlightForeground>
        <selectionBorder>#222218</selectionBorder>
        <activeGuide>#9D550FB0</activeGuide>
        <bracketsForeground>#F8F8F2A5</bracketsForeground>
        <bracketsOptions>underline</bracketsOptions>
        <bracketContentsForeground>#F8F8F2A5</bracketContentsForeground>
        <bracketContentsOptions>underline</bracketContentsOptions>
        <tagsOptions>stippled_underline</tagsOptions>
    </globals>
    <rules>
        <comment>
            <foreground>#6272a4</foreground>
        </comment>
        <string>
            <foreground>#f1fa8c</foreground>
        </string>
        <constant.numeric>
            <foreground>#bd93f9</foreground>
        </constant.numeric>
        <constant.language>
            <foreground>#bd93f9</foreground>
        </constant.language>
        <constant.character>
            <foreground>#bd93f9</foreground>
        </constant.character>
        <constant.other>
            <foreground>#bd93f9</foreground>
        </constant.other>
        <variable>
        </variable>
        <variable.other.readwrite.instance>
            <foreground>#ffb86c</foreground>
        </variable.other.readwrite.instance>
        <constant.character.escaped>
