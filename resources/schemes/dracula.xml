            <foreground>#ff79c6</foreground>
        </punctuation.separator.namespace>
        <variable.function>
            <foreground>#50fa7b</foreground>
        </variable.function>
        <variable.other>
            <foreground>#ffffff</foreground>
        </variable.other>
        <variable.language>
            <foreground>#bd93f9</foreground>
        </variable.language>
        <entity.name.module.ruby>
            <foreground>#8be9fd</foreground>
        </entity.name.module.ruby>
        <entity.name.constant.ruby>
            <foreground>#bd93f9</foreground>
        </entity.name.constant.ruby>
        <support.function.builtin.ruby>
            <foreground>#ffffff</foreground>
        </support.function.builtin.ruby>
        <storage.type.namespace.cs>
            <foreground>#ff79c6</foreground>
        </storage.type.namespace.cs>
        <entity.name.namespace.cs>
            <foreground>#8be9fd</foreground>
        </entity.name.namespace.cs>
